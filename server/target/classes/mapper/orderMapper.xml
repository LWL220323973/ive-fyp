<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.server.mapper.OrderMapper">
    <select id="findAllOrders" resultType="com.server.model.Order"> SELECT * FROM `order` </select>

    <select id="findOrdersByTableName" parameterType="string" resultType="com.server.model.Order">
        SELECT * FROM `order` WHERE table_name = #{tableName} </select>
</mapper>